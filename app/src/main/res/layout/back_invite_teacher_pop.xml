<?xml version="1.0" encoding="utf-8"?>
<www.linwg.org.lib.LCardView xmlns:android="http://schemas.android.com/apk/res/android"
                             xmlns:app="http://schemas.android.com/apk/res-auto"
                             android:id="@+id/clander_layout"
                             android:layout_width="@dimen/m311"
                             android:layout_height="@dimen/m161"
                             android:layout_gravity="center"
                             android:gravity="center"
                             android:orientation="vertical"
                             app:cornerRadius="@dimen/m8"
                             app:shadowSize="@dimen/m2"
                             app:shadowStartAlpha="10">


    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

        <TextView
                android:id="@+id/pop_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/m10"
                android:text="@string/tv176"
                android:textColor="@color/color_4A4A4A"
                android:textSize="@dimen/sp18"/>

        <TextView
                android:id="@+id/pop_content"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/pop_title"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="@dimen/m53"
                android:layout_marginTop="@dimen/m10"
                android:layout_marginRight="@dimen/m32"
                android:gravity="left"
                android:text="@string/tv186"
                android:textColor="@color/color_4A4A4A"
                android:textSize="@dimen/sp15"/>

        <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="@dimen/m15">


            <LinearLayout
                    android:id="@+id/cancel_btn"
                    android:layout_width="@dimen/m130"
                    android:layout_height="@dimen/m39"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/m10"
                    android:layout_marginRight="@dimen/m15"
                    android:background="@drawable/button_cancle"
                    android:gravity="center">

                <TextView
                        android:id="@+id/dissmiss_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/alert_dialog_cached_cancel"
                        android:textColor="@color/white"
                        android:textSize="@dimen/sp18"/>
            </LinearLayout>

            <LinearLayout
                    android:id="@+id/sure_btn"
                    android:layout_width="@dimen/m130"
                    android:layout_height="@dimen/m39"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/m10"
                    android:background="@drawable/cancel_bg"
                    android:gravity="center">


                <TextView
                        android:id="@+id/sure_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tv189"
                        android:textColor="@color/white"
                        android:textSize="@dimen/sp18"/>
            </LinearLayout>
        </RelativeLayout>

    </RelativeLayout>


</www.linwg.org.lib.LCardView>
