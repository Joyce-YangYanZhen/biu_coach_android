<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_marginTop="@dimen/m5"
              android:layout_height="match_parent">
    <www.linwg.org.lib.LCardView
            android:layout_width="match_parent"
            android:orientation="vertical"
            app:shadowStartAlpha="10"
            app:cornerRadius="@dimen/m8"
            app:shadowSize="@dimen/m2"
            android:gravity="center"
            android:layout_height="wrap_content">

        <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_marginLeft="@dimen/m20"
                android:layout_marginRight="@dimen/m20"
                android:layout_height="wrap_content">

            <RelativeLayout
                    android:layout_marginTop="@dimen/m15"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content">

                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tv33"
                        android:textSize="15sp"
                        android:layout_centerVertical="true"
                        android:textColor="@color/top_heiziti"
                        />

                <LinearLayout
                        android:layout_alignParentLeft="true"
                        android:layout_width="match_parent"
                        android:layout_marginLeft="@dimen/m70"
                        android:background="@drawable/edit_bg"
                        android:layout_centerVertical="true"
                        android:layout_height="@dimen/m31">

                    <EditText
                            android:layout_width="match_parent"
                            android:hint="@string/tv34"
                            android:background="@null"
                            android:paddingLeft="@dimen/m20"
                            android:textColorHint="@color/color_929292"
                            android:textColor="@color/top_heiziti"
                            android:textSize="15sp"
                            android:layout_height="match_parent"/>
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="@dimen/m20"
                    android:layout_height="wrap_content">

                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tv35"
                        android:textSize="15sp"
                        android:layout_centerVertical="true"
                        android:textColor="@color/top_heiziti"
                        />

                <LinearLayout
                        android:layout_alignParentLeft="true"
                        android:layout_width="match_parent"
                        android:layout_marginLeft="@dimen/m70"
                        android:layout_centerVertical="true"
                        android:background="@drawable/edit_bg"
                        android:layout_height="@dimen/m31">

                    <EditText
                            android:layout_width="match_parent"
                            android:hint="@string/tv36"
                            android:background="@null"
                            android:paddingLeft="@dimen/m20"
                            android:textColorHint="@color/color_929292"
                            android:textColor="@color/top_heiziti"
                            android:textSize="15sp"
                            android:layout_height="match_parent"/>
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="@dimen/m20"
                    android:layout_marginBottom="@dimen/m15"
                    android:layout_height="wrap_content">

                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tv37"
                        android:textSize="15sp"
                        android:layout_centerVertical="true"
                        android:textColor="@color/top_heiziti"
                        />

                <RelativeLayout
                        android:id="@+id/select_sex_btn"
                        android:layout_alignParentLeft="true"
                        android:layout_width="match_parent"
                        android:layout_marginLeft="@dimen/m70"
                        android:background="@drawable/edit_bg"
                        android:layout_centerVertical="true"
                        android:layout_height="@dimen/m31">

                    <TextView
                            android:layout_width="wrap_content"
                            android:hint="@string/tv38"
                            android:background="@null"
                            android:enabled="false"
                            android:gravity="center_vertical"
                            android:paddingLeft="@dimen/m20"
                            android:layout_gravity="center_vertical"
                            android:textColorHint="@color/color_929292"
                            android:textColor="@color/top_heiziti"
                            android:textSize="15sp"
                            android:layout_height="match_parent"/>
                    <ImageView
                            android:layout_width="@dimen/m14"
                            android:layout_height="@dimen/m8"
                            android:layout_centerVertical="true"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="@dimen/m20"
                            android:layout_gravity="right|center_vertical"
                            android:src="@drawable/xiala"
                            />

                </RelativeLayout>
            </RelativeLayout>

        </LinearLayout>
    </www.linwg.org.lib.LCardView>

    <www.linwg.org.lib.LCardView
            android:layout_width="match_parent"
            android:orientation="vertical"
            app:shadowStartAlpha="10"
            android:layout_marginTop="@dimen/m5"
            app:cornerRadius="@dimen/m8"
            app:shadowSize="@dimen/m2"
            android:gravity="center"
            android:layout_height="wrap_content">

        <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_marginLeft="@dimen/m20"
                android:layout_marginRight="@dimen/m20"
                android:layout_height="wrap_content">

            <RelativeLayout
                    android:layout_marginTop="@dimen/m15"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content">

                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tv39"
                        android:textSize="15sp"
                        android:layout_centerVertical="true"
                        android:textColor="@color/top_heiziti"
                        />

                <LinearLayout
                        android:layout_alignParentLeft="true"
                        android:layout_width="match_parent"
                        android:layout_marginLeft="@dimen/m70"
                        android:background="@drawable/edit_bg"
                        android:layout_centerVertical="true"
                        android:layout_height="@dimen/m31">

                    <EditText
                            android:layout_width="match_parent"
                            android:hint="@string/tv40"
                            android:background="@null"
                            android:paddingLeft="@dimen/m20"
                            android:textColorHint="@color/color_929292"
                            android:textColor="@color/top_heiziti"
                            android:textSize="15sp"
                            android:layout_height="match_parent"/>
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="@dimen/m20"
                    android:layout_height="wrap_content">

                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tv41"
                        android:textSize="15sp"
                        android:layout_centerVertical="true"
                        android:textColor="@color/top_heiziti"
                        />

                <RelativeLayout
                        android:id="@+id/select_address_btn"
                        android:layout_alignParentLeft="true"
                        android:layout_width="match_parent"
                        android:layout_marginLeft="@dimen/m70"
                        android:background="@drawable/edit_bg"
                        android:layout_centerVertical="true"
                        android:layout_height="@dimen/m31">

                    <TextView
                            android:layout_width="wrap_content"
                            android:hint="@string/tv42"
                            android:background="@null"
                            android:enabled="false"
                            android:gravity="center_vertical"
                            android:paddingLeft="@dimen/m20"
                            android:layout_gravity="center_vertical"
                            android:textColorHint="@color/color_929292"
                            android:textColor="@color/top_heiziti"
                            android:textSize="15sp"
                            android:layout_height="match_parent"/>
                    <ImageView
                            android:layout_width="@dimen/m14"
                            android:layout_height="@dimen/m8"
                            android:layout_centerVertical="true"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="@dimen/m20"
                            android:layout_gravity="right|center_vertical"
                            android:src="@drawable/xiala"
                            />

                </RelativeLayout>

            </RelativeLayout>
            <RelativeLayout
                    android:layout_marginTop="@dimen/m20"
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_height="wrap_content">

                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tv44"
                        android:textSize="15sp"
                        android:layout_centerVertical="true"
                        android:textColor="@color/top_heiziti"
                        />

                <LinearLayout
                        android:layout_alignParentLeft="true"
                        android:layout_width="match_parent"
                        android:layout_marginLeft="@dimen/m70"
                        android:background="@drawable/edit_bg"
                        android:layout_centerVertical="true"
                        android:layout_height="@dimen/m31">

                    <EditText
                            android:layout_width="match_parent"
                            android:hint="@string/tv45"
                            android:background="@null"
                            android:paddingLeft="@dimen/m20"
                            android:textColorHint="@color/color_929292"
                            android:textColor="@color/top_heiziti"
                            android:textSize="15sp"
                            android:layout_height="match_parent"/>
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                    android:layout_width="match_parent"
                    android:orientation="horizontal"
                    android:layout_marginTop="@dimen/m20"
                    android:layout_marginBottom="@dimen/m15"
                    android:layout_height="wrap_content">

                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tv43"
                        android:textSize="15sp"
                        android:layout_centerVertical="true"
                        android:textColor="@color/top_heiziti"
                        />

                <RelativeLayout
                        android:id="@+id/time_select_btn"
                        android:layout_alignParentLeft="true"
                        android:layout_width="match_parent"
                        android:layout_marginLeft="@dimen/m70"
                        android:background="@drawable/edit_bg"
                        android:layout_centerVertical="true"
                        android:layout_height="@dimen/m31">

                    <TextView
                            android:layout_width="wrap_content"
                            android:hint="@string/tv38"
                            android:background="@null"
                            android:enabled="false"
                            android:gravity="center_vertical"
                            android:paddingLeft="@dimen/m20"
                            android:layout_gravity="center_vertical"
                            android:textColorHint="@color/color_929292"
                            android:textColor="@color/top_heiziti"
                            android:textSize="15sp"
                            android:layout_height="match_parent"/>
                    <ImageView
                            android:layout_width="@dimen/m14"
                            android:layout_height="@dimen/m8"
                            android:layout_centerVertical="true"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="@dimen/m20"
                            android:layout_gravity="right|center_vertical"
                            android:src="@drawable/xiala"
                            />

                </RelativeLayout>
            </RelativeLayout>

        </LinearLayout>
    </www.linwg.org.lib.LCardView>
</LinearLayout>